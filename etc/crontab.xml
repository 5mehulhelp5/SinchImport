<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">
        <job name="sinchimport_full" instance="Magebuzz\Sinchimport\Cron\Importfull" method="execute">
            <schedule>0 21 * * 1</schedule>
        </job>
        <job name="sinchimport_stockprice" instance="Magebuzz\Sinchimport\Cron\Importstockprice" method="execute">
            <schedule>0 8 * * *</schedule>
        </job>
    </group>
</config>
